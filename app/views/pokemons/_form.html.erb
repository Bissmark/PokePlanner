<%= form_for @pokemon do |f| %>
    <%= f.label :name  %>
    <%= f.text_field :name %>

    <%= f.label :weight %>
    <%= f.text_field :weight %>

    <%= f.label :height %>
    <%= f.text_field :height %>

    <%= f.label :description %>
    <%= f.text_field :description %>    

    <%= f.label :image %>
    <%= f.text_field :image, :required => true %>

    <%= f.submit %>
<% end %>